SFX=
ifeq ($(GOOS),windows)
SFX:=.exe
TARG:= $(foreach t,$(TARG),$(t)$(SFX))
endif

all: $(TARG)

include $(QUOTED_GOROOT)/src/Make.common

PREREQ+=$(patsubst %,%.make,$(DEPS))

%$(SFX): %.$O
	$(LD) -o $@ $<

%.$O: %.go $(PREREQ)
	$(GC) -o $@ $<

CLEANFILES+=$(TARG)

importpath:
	echo main
